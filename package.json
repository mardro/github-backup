{
  "name": "github-repo-backup",
  "version": "1.0.1",
  "description": "A NodeJs CLI tool to backup GitHub repositories",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node .index.js backup -d PATH_TO_BACKUP_DIR -r all -o ORGANISATION_NAME -p GITHUB_PAT"
  },
  "bin": {
    "github-repo-backup": "./index.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mardro/github-repo-backup.git"
  },
  "keywords": [
    "github",
    "backup",
    "cli"
  ],
  "author": "Mark Drobnick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mardro/github-repo-backup/issues"
  },
  "homepage": "https://github.com/mardro/github-repo-backup#readme",
  "dependencies": {
    "@octokit/core": "^4.0.4",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^9.2.0",
    "figlet": "^1.5.2"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "nodemon": "^2.0.16"
  }
}
